<?xml version='1.0' encoding='UTF-8'?>
<soap:Envelope {% for ns, link in NAMESPACES.items %}xmlns:{{ns}}="{{link}}" {% endfor %}>
    <soap:Header>
        <wsa:Action wsu:Id="Id-Action">http://docs.oasis-open.org/ws-sx/ws-trust/200512/RST/Issue</wsa:Action>
        <wsa:MessageID wsu:Id="Id-MessageID">urn:uuid:{{MessageID}}</wsa:MessageID>
        <wsa:To wsu:Id="Id-To">https://ws.ite.realme.govt.nz/iCMS/Validate_v1_1</wsa:To>
        <wsa:ReplyTo wsu:Id="Id-ReplyTo">
            <wsa:Address>http://www.w3.org/2005/08/addressing/anonymous</wsa:Address>
        </wsa:ReplyTo>
        <wsse:Security soap:mustUnderstand="1">

            <wsu:Timestamp wsu:Id="Id-Timestamp">
                <wsu:Created>{{Created}}</wsu:Created>
                <wsu:Expires>{{Expires}}</wsu:Expires>
            </wsu:Timestamp>

            <ds:Signature>
                <ds:SignedInfo>
                    <ds:CanonicalizationMethod Algorithm="http://www.w3.org/2001/10/xml-exc-c14n#" />
                    <ds:SignatureMethod Algorithm="http://www.w3.org/2000/09/xmldsig#rsa-sha1" />
                    {% for ref_id in REF_IDS %}
                    <ds:Reference URI="#{{ref_id}}">
                        <ds:Transforms>
                            <ds:Transform Algorithm="http://www.w3.org/2001/10/xml-exc-c14n#">
                                <ec:InclusiveNamespaces PrefixList="soap" />
                            </ds:Transform>
                        </ds:Transforms>
                        <ds:DigestMethod Algorithm="http://www.w3.org/2001/04/xmlenc#sha256" />
                        <ds:DigestValue/>
                    </ds:Reference>
                    {% endfor %}
                </ds:SignedInfo>
                <ds:SignatureValue/>
                <ds:KeyInfo></ds:KeyInfo>
            </ds:Signature>

        </wsse:Security>
    </soap:Header>
    <soap:Body wsu:Id="Id-Body">
        TODO
    </soap:Body>
</soap:Envelope>
